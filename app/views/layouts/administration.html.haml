                          %input#male.required{:name => "gender", :type => "radio"}/
                          %label{:for => "male"} Male
                        %li
                          %input#female{:name => "gender", :type => "radio"}
                            %label{:for => "female"} Female
                      %label.error.plain{:for => "gender", :generated => "true", :style => "display:none"}
                %fieldset#step-2.mws-form-inline
                  %legend.wizard-label
                    %i.icol-delivery
                    Membership Type
                  .mws-form-row{:id => ""}
                    %label.mws-form-label
